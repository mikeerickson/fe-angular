{
  "parser": "babel-eslint",
  "ecmaFeatures": {
      "blockBindings": true,
      "forOf": true,
      "jsx": true,
      "classes": true
  },
  "rules": {
    "global-strict": 0,
    "no-console": 0,
    "semi": 0,
    "quote": [0, "single"],
    "quotes": [0, "single"],
    "no-undef": 0,
    "no-multi-spaces": 0,
    "eol-last": 0,
    "no-redeclare": 0,
    "comma-spacing": 0
  },
  "global": {
    "require": 0
  }
}
